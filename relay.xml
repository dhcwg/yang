<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <dhcpv6-relay xmlns="urn:ietf:params:xml:ns:yang:ietf-dhcpv6-relay">
    <enabled>true</enabled>
    <relay-if>
      <if-name xmlns="urn:ietf:params:xml:ns:yang:ietf-interfaces">
        eth0
      </if-name>
      <enabled>true</enabled>
      <destination-address>2001:db8:2::1</destination-address>
      <destination-address>2001:db8:2::2</destination-address>
      <link-address>2001:db8:3::1</link-address>
      <relay-options>
        <interface-id-option>
         <interface-id>EXAMPLE-INTERFACE-ID</interface-id>
        </interface-id-option>
      </relay-options>
      <solicit-received-count/>
      <advertise-sent-count/>
      <request-received-count/>
      <confirm-received-count/>
      <renew-received-count/>
      <rebind-received-count/>
      <reply-sent-count/>
      <release-received-count/>
      <decline-received-count/>
      <reconfigure-sent-count/>
      <information-request-received-count/>
      <unknown-message-received-count/>
      <unknown-message-sent-count/>
      <discarded-message-count/>
      <prefix-delegation>
        <pd-leases>
          <ia-pd-prefix/>
          <last-renew/>
          <client-peer-address/>
          <client-duid/>
          <server-duid/>
        </pd-leases>
      </prefix-delegation>
    </relay-if>
    <relay-forward-sent-count/>
    <relay-forward-received-count/>
    <relay-reply-received-count/>
    <relay-forward-unknown-sent-count/>
    <relay-forward-unknown-received-count/>
    <discarded-message-count/>
  </dhcpv6-relay>
</data>
